<% layout('/layouts/boilerplate') %>
    
    <H1> Please sign up to add and delete images from this repository!</H1>
    <H1> All images (Select the image to view it)</H1>
    <div>
        <a href="/images/new">Add a new image</a>
    </div>
        <ul>
            <% for (let image of images) { %>
                <li>
                    <a href="/images/<%= image._id %>"> 
                         <%= image.title %>  
                    </a>
                </li>
           <% }  %>

        </ul>
